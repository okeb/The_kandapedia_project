<div class="container">
<%= turbo_frame_tag dom_id(question, "edit"), data:{turbo_action: "advance"} do %>
  <div style="width:100%; height:50px"></div>

  <div id="<%= dom_id question %>"> 

    <%= render "title", as: :question %>


    <%= render "body" %>
    
  
    

    <%= link_to "Edit this question", edit_question_path(question) %>

  </div> 

  <% end %>
</div>

<%= form_with url: rodauth.email_auth_request_path, method: :post, data: { turbo: false }, class:"form__container" do |form| %>
  <%= form.hidden_field rodauth.login_param, value: params[rodauth.login_param] %>

  <div class="email_auth_request">
    <%= form.submit rodauth.email_auth_request_button, class: "btn btn-_primary" %>
  </div>
<% end %>

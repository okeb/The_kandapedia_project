              <p class="name_author">
                <%= (!@kandy.account.profile.lastname.nil? || !@kandy.account.profile.firstname.nil?) ? "#{@kandy.account.profile.lastname.capitalize} #{@kandy.account.profile.firstname.capitalize}" : @kandy.account.profile.username.capitalize %>

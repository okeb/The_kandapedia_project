              <p class="username_author" style="opacity:.3; font-size: x-small; margin-top: auto; margin-bottom: auto">&bull;</p>
              <p class="username_author" style="opacity: 1; font-weight: 500; font-size: small; margin-top: auto; margin-bottom: auto;margin-left: 0; padding-left:8px; padding-right: 8px; color:var(--color-primary)">
                <%= link_to 'Suivre', follow_profile_path(@kandy.account) %>
              </p>
            <% end %>
          <% end %>
        </div>
        <p class="" style="font-size:15px; background-color: transparent; line-height: 21px;"><%= link_to @kandy.body, candy_url(@kandy), style:"text-decoration:none; color:var(--color-text-body-high); font-size:1rem" %>
        </p>
        <% if @kandy.image_data? %>
          <div class="image" style="width: 100%; border-radius: 21px; overflow: hidden; margin: 14px 0;">
              <%= image_tag @kandy.image_url(:medium), style: "object-fit: cover; object-position: center; max-width:100%; max-height:540px; " %>
          </div>
        <% end %>

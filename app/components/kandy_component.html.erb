<div>
    <%= content_tag :div, style:"display:flex; justify-content:start; column-gap: 14px; margin: 0; margin-top:-1px; padding: 12px 24px 0 24px; border-bottom:1px solid var(--color-border); padding: 24px 1.25rem" do %>
      <%= button_to candy_author_url(@kandy), style: 'text-decoration:none; border-radius:7px; background-color:transparent; border-color:transparent; cursor:pointer', method: :put do %>
        <div style=" display:flex; align-items:center;  justify-content: center;margin:1.5px">
          <% if @kandy.account.profile.avatar.attached? %>
            <% if @kandy.account.type == 'user' %>
              <div class="picture_author" style="height: 47px; width: 47px; margin:0;">
                <%= image_tag(@kandy.account.profile.avatar_profile, style: "object-fit: cover; object-position: center;width:100%; height:100%; border-radius:100%", size:"27px") %>
              </div>
            <% else %>
              <div style="height:47px; width:47px;">
                <div class="maskSquircle__large">
                  <%= image_tag(@kandy.account.profile.avatar_profile) %>
                </div>
              </div>
            <% end %>
          <% else %>
            <div class="initial_author"
                style="background-color:<%= @kandy.account.profile.color %>; height:47px; width:47px;">
              <p style="font-size:18px">
                <%= @kandy.account.profile.get_initial_profile %>
              </p>
            </div>
          <% end %>
        </div>
      <% end %>
      
      <div style="border-radius:7px; width:100%; margin:0; padding: 0;">
        <div style="display:flex; column-gap: 3px;margin-bottom:7px">
